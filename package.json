{
  "name": "treachery.bot",
  "version": "1.0.0",
  "description": "MTG Treachery Discord Bot",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development rollup -c --watch",
    "build": "NODE_ENV=production rollup -c",
    "package": "docker build -t treachery-bot:latest dist",
    "lint": "concurrently npm:lint:*",
    "lint:prettier": "prettier . --check",
    "lint:depcheck": "depcheck",
    "format": "prettier . --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": {
    "run": "lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HunterLarco/treachery.bot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HunterLarco/treachery.bot/issues"
  },
  "homepage": "https://github.com/HunterLarco/treachery.bot#readme",
  "dependencies": {
    "ajv": "8.12.0",
    "discord.js": "14.8.0",
    "dotenv": "16.0.3",
    "dynamoose": "2.8.5",
    "express": "4.18.2",
    "fast-glob": "3.2.4",
    "node-fetch": "2.6.7",
    "pick-random": "4.0.0",
    "shuffle-array": "1.0.1",
    "tslib": "2.5.0",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-run": "3.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@types/express": "4.17.17",
    "@types/node-fetch": "2.6.3",
    "@types/shuffle-array": "1.0.2",
    "@types/uuid": "9.0.1",
    "concurrently": "7.6.0",
    "depcheck": "1.4.3",
    "pre-commit": "1.2.2",
    "prettier": "2.1.2",
    "rollup": "3.3.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-generate-package-json": "3.2.0",
    "typescript": "4.7.2"
  }
}
